{
  "extends": "@vue-storefront/integrations-tsconfig/tsconfig",
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es5",
    "module": "ES2015",
    "importHelpers": true,
    "noEmitHelpers": true,
    "sourceMap": true,
    "baseUrl": "./src",
    "lib": ["es6", "es7", "ES2017", "ES2018", "ES2019", "dom"],
    "strict": false,
  },
  "exclude": ["node_modules", "**/*.spec.ts", "*.js", "server", "lib"]
}
